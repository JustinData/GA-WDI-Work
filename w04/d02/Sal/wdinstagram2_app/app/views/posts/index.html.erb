<div class="container">
	<h1>Instagrams</h1>
	<h2><a href="/posts/new">Create a new Instagram</a></h2>

	<% @posts.each do |post| %>
	<section>
		<h2><%= post.author %></h2>
		<p><%= post.date_taken %></p>
		<a href="/posts/<%= post.id %>"><img src="<%= post.photo_url %>"></a>
	<section>
	<% end %>
</div>

