 <!-- for each movie in the all_movies array split into elements and store into a movie array. routes to movies.erb -->
 <body id="movie">
  
   
 <% @results.each do |row| %>
 <section>
 <h2><a href="/movies/<%= row["id"]%>">Entry:</a></h2>
    <ul>
      <li>Year: <%= row["year"] %></li>
      <li><img src="<%= row["poster"]%>"></li>
      <li>Title: <%= row["title"] %></li>
    </ul>
  </section>
</body>

            

  <% end %>

  